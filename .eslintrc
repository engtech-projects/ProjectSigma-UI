{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "@nuxtjs/eslint-config-typescript"
    ],
    "rules": {
        "quotes": [1, "double"],
        "indent": [2, 4],
        "vue/html-indent": [2, 4],
        "comma-dangle": [2, {
                "arrays": "only-multiline",
                "objects": "only-multiline"
            }
        ],
        //  Below Will be Temporary
        "vue/require-prop-types": 0,
        "@typescript-eslint/no-mutating-props": 0,
        "vue/no-mutating-props": 0,
        "camelcase": 0,
        "no-console": 0
    }
}